{
	"if": {
		"anyOf": [{
				"not": {
					"anyOf": [{
							"field": "type",
							"like": "microsoft.alertsmanagement/smartDetectorAlertRules/*"
						},
						{
							"field": "type",
							"like": "Microsoft.EventGrid/topics/*"
						},
						{
							"field": "type",
							"like": "Microsoft.CognitiveServices/accounts/*"
						},
						{
							"field": "type",
							"like": " Microsoft.Web/serverfarms/*"
						},
						{
							"field": "type",
							"like": "microsoft.insights/actiongroups/*"
						},
						{
							"field": "type",
							"like": "Microsoft.Insights/components/*"
						},
						{
							"field": "type",
							"like": "Microsoft.EventGrid/systemTopics/*"
						},
						{
							"field": "type",
							"like": "Microsoft.KeyVault/vaults/*"
						},
						{
							"field": "type",
							"like": "Microsoft.ServiceBus/namespaces/*"
						},
						{
							"field": "type",
							"like": "Microsoft.Sql/servers/databases/*"
						},
						{
							"field": "type",
							"like": "Microsoft.Sql/servers/*"
						},
						{
							"field": "type",
							"like": "Microsoft.Storage/storageAccounts/*"
						},
						{
							"field": "type",
							"like": "Microsoft.Web/sites/*"
						},
						{
							"field": "type",
							"in": ["microsoft.alertsmanagement/smartDetectorAlertRules", "Microsoft.EventGrid/topics", "Microsoft.Web/serverfarms", "Microsoft.CognitiveServices/accounts", "microsoft.insights/actiongroups", "Microsoft.EventGrid/systemTopics", "Microsoft.Insights/components", "Microsoft.KeyVault/vaults", "Microsoft.ServiceBus/namespaces", "Microsoft.Sql/servers/databases", "Microsoft.Sql/servers/*", "Microsoft.Storage/storageAccounts", "Microsoft.Web/sites"]
						}
					]
				}
			},
			{
				"allof": [{
						"field": "type",
						"equals": "Microsoft.KeyVault/vaults"
					},
					{
						"field": "Microsoft.KeyVault/vaults/sku.name",
						"notIn": ["Standard"]
					}
				]
			},
			{
				"allof": [{
						"field": "type",
						"equals": "Microsoft.sql/servers"
					},
					{
						"source": "action",
						"equals": "Microsoft.sql/servers/write"
					}
				]
			},

			{
				"allOf": [{
						"source": "action",
						"equals": "Microsoft.Storage/storageAccounts/write"
					},
					{
						"field": "type",
						"equals": "Microsoft.Storage/storageAccounts"
					},
					{
						"not": {
							"field": "Microsoft.Storage/storageAccounts/sku.name",
							"in": ["Standard_GRS"]
						}
					}
				]
			}

		]
	},
	"then": {
		"effect": "deny"
	}
}
